<h1><%= title %></h1>
<p>Welcome to <%= title %></p>

<button>start</button>

<hr>
<b>discovered services:</b>
<br><output><pre></pre></output>


<script src="/javascripts/discovery.js"></script>

<script>
var output = document.querySelector("pre");

document.querySelector("button").onclick = function(){
  console.log("start");
  output.innerText = "探索中・・・";
  navigator.getNetworkServices('urn:....', show)
}

function show(lists) {
  console.log(lists);
  var services = lists.getServices();
  output.innerText = JSON.stringify(services, null, 2);
}
</script>
